page{
    background: #eeeeee;
}
.orders-address{
    height: 157rpx;
    padding: 0 34rpx 7rpx 34rpx;
    background: #ffffff;
    background-image: url(data:image/jpeg;base64,);
    background-repeat: repeat-x;
    background-position: left bottom;
    background-size: 78rpx 7rpx;
    margin-top: 5rpx;
    position: relative;
}
.orders-address:after{
    content:'';
    width: 10rpx;
    height: 10rpx;
    border-top: 1px solid #ccc;
    border-right: 1px solid #ccc;
    transform: rotate(45deg);
    position: absolute;
    right: 30rpx;
    top: 80rpx;
}
.orders-no-address{
    line-height: 157rpx;
}