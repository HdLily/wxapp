page{
    background: #eee;
}
.orders-address{
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -100rpx;
    margin-top: -200rpx;
    color: #999;
}
.orders-address image{
    width: 200rpx;
    height: 200rpx;
}
.addNewAddress{
    position: fixed;
    left: 0;
    bottom: 0;
    height: 100rpx;
    width: 100%;
    line-height: 100rpx;
    text-align: center;
    background: #ffffff;
    color: #999;
}
.addNewAddress image{
    width: 40rpx;
    height: 40rpx;
    margin-right: 30rpx;
}