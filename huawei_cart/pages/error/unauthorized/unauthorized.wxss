.user-info {
    padding: 109rpx 0 94rpx;
}

.warrant-img {
    text-align: center;
    padding-bottom: 32rpx;
}

.warrant-img image {
    width: 250rpx;
    height: 250rpx;
}

.warrant-result {
    font-size: 38rpx;
    color: #666;
    text-align: center;
    padding-bottom: 36rpx;
}

.tips-warrant {
    font-size: 25rpx;
    color: #999;
    text-align: center;
    line-height: 1.4;
}

.user-action {
    margin: 0 34rpx;
    border: 1px solid #ddd;
    box-shadow: 0 0 11rpx #ddd;
}

.user-action-title {
    font-size: 28rpx;
    color: #666;
    height: 94rpx;
    padding: 0 32rpx;
}

.user-action-box {
    font-size: 32rpx;
    height: 94rpx;
    padding: 0 32rpx;
    border-bottom: 3rpx solid #ddd;
}

.user-action-box:last-child {
    border-bottom: 0;
}

.switch {
    width: 67rpx;
    height: 34rpx;
    background-repeat: no-repeat;
    background-position: center;
    background-size: 67rpx 34rpx;
    position: relative;
}

.switch_open {
    background-image: url(data:image/png;base64,);
}

.switch_close {
    background-image: url(data:image/png;base64,);
}

.switch image {
    width: 34rpx;
    height: 34rpx;
    position: absolute;
    left: 0;
    top: 0;
}

.switch_open image {
    animation: switch_open 1s;
    animation-fill-mode: forwards;
}

.switch_close image {
    animation: switch_close 1s;
    animation-fill-mode: forwards;
}

@-webkit-keyframes switch_open {
    0% {
        left: 0px;
    }

    100% {
        left: 50%;
    }
}

@keyframes switch_open {
    0% {
        left: 0px;
    }

    100% {
        left: 50%;
    }
}

@-webkit-keyframes switch_close {
    0% {
        left: 50%;
    }

    100% {
        left: 0;
    }
}

@keyframes switch_close {
    0% {
        left: 50%;
    }

    100% {
        left: 0;
    }
}

.user-guide {
    font-size: 23rpx;
    color: #999;
    line-height: 1.2;
    margin-top: 27rpx;
    padding: 0 34rpx;
    display: -ms-flexbox;
    display: box;
    display: -webkit-flex;
    display: flex;
    align-items: center;
}

.user-guide image {
    width: 21rpx;
    height: 21rpx;
    margin-right: 8rpx;
}