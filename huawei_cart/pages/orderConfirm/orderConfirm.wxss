.order-form-area {
    margin-bottom: 13rpx;
}

.order-confirm-address {
    height: 157rpx;
    padding: 0 34rpx 7rpx 34rpx;
    background-image: url(data:image/jpeg;base64,);
    background-repeat: repeat-x;
    background-position: left bottom;
    background-size: 78rpx 7rpx;
}

.order-confirm-address .icon-location {
    width: 38rpx;
    height: 38rpx;
    padding-right: 11rpx;
}

.order-confirm-address .icon-location image {
    width: 38rpx;
    height: 38rpx;
}

.order-confirm-address .address-cont {
}

.order-confirm-address .addr-user {
    font-size: 28rpx;
}

.order-confirm-address .addr-user-name {
    font-weight: bold;
    padding-right: 21rpx;
}

.order-confirm-address .addr-info {
    font-size: 25rpx;
    line-height: 1.3;
    padding-top: 11rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    word-break: break-all;
}

.order-confirm-address .addr-empty {
    font-size: 32rpx;
    font-weight: bold;
}

.order-confirm-address .btn-next {
    padding-left: 21rpx;
}

.o-invoice {
    font-size: 28rpx;
    height: 84rpx;
    padding: 0 34rpx;
}

.o-invoice .invoice-title {
    color: #888;
    padding: 0 9rpx 0 32rpx;
    display: block;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.order-confirm-header {
    font-size: 28rpx;
    font-weight: bold;
    min-height: 84rpx;
    padding: 0 34rpx;
}

.order-confirm-shipper {
    padding: 13rpx 34rpx;
    min-height: inherit;
}

.order-confirm-shipper .system-item-center {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    position: relative;
    padding-left: 47rpx;
}

.order-confirm-header .icon-shipper {
    width: 38rpx;
    height: 38rpx;
    padding-right: 9rpx;
    position: absolute;
    left: 0;
    top: 50%;
    margin-top: -16rpx;
}

.order-confirm-header .icon-shipper image {
    width: 38rpx;
    height: 38rpx;
}

.order-confirm-header .order-header-small {
    font-size: 23rpx;
    font-weight: normal;
    padding-left: 17rpx;
}

.order-confirm-shipper .order-arrived {
    font-size: 25rpx;
    font-weight: normal;
}

.order-confirm-shipper .icon-query {
    width: 38rpx;
    height: 38rpx;
    padding-left: 11rpx;
}

.order-confirm-shipper .icon-query image {
    width: 38rpx;
    height: 38rpx;
}

.order-good-area .g-tip {
    font-size: 23rpx;
    padding-top: 3rpx;
}

.order-good-area .g-summary {
    font-size: 25rpx;
    min-width: 188rpx;
    padding-left: 21rpx;
}

.invalid-good .g-summary {
    min-width: auto;
}

.order-good-area .g-summary .g-amount {
    color: #888;
}

.order-good-area .g-summary .g-price {
    padding-left: 21rpx;
}

.order-good-box {
    padding: 15rpx 34rpx;
}

.invalid-good .order-good-box {
    padding: 21rpx 0;
    border-top: 1px solid #eaeaea;
}

.invalid-good .order-good-box:first-of-type {
    border-top: 0;
}

.order-good-box .g-img {
    width: 115rpx;
    height: 115rpx;
    padding-right: 17rpx;
}

.order-good-box .g-img image {
    width: 115rpx;
    height: 115rpx;
}

.order-good-box .g-basic {
    flex-wrap: wrap;
}

.order-good-box .g-basic .g-name {
    width: 100%;
    font-size: 25rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.order-good-annex {
    padding: 0 34rpx;
    background-color: #fbfbfb;
}

.order-annex-list {
    padding: 11rpx 0 11rpx 84rpx;
    position: relative;
}

.order-good-module.disabled {
    color: #888;
    background-color: #fbfbfb;
}

.order-annex-title {
    font-size: 23rpx;
    color: #888;
    padding: 21rpx 0;
    position: absolute;
    top: 0;
    left: 0;
}

.order-annex-service .order-annex-title {
    padding: 15rpx 0;
}

.order-annex-box {
    padding: 7rpx 0;
}

.order-annex-service .order-annex-box {
    padding: 5rpx 0;
}

.order-annex-box.disabled {
    color: #888;
}

.order-annex-box .g-img {
    width: 40rpx;
    height: 40rpx;
    margin-right: 17rpx;
    border: 3rpx solid #eaeaea;
}

.order-annex-box .g-img image {
    width: 40rpx;
    height: 40rpx;
}

.order-annex-box .g-name {
    font-size: 23rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.order-confirm-deliver {
    font-size: 25rpx;
    height: 84rpx;
    padding: 0 34rpx;
}

.order-confirm-summary {
    line-height: 1.4;
    padding: 23rpx 34rpx;
}

.order-summary-title {
    font-size: 25rpx;
}

.order-summary-cont {
    font-size: 32rpx;
    min-width: 188rpx;
    text-align: right;
}

.order-summary-small {
    font-size: 23rpx;
}

.order-confirm-coupon {
    padding: 0 34rpx;
    height: 84rpx;
}

.icon-coupon {
    width: 38rpx;
    height: 38rpx;
    padding-right: 11rpx;
}

.icon-coupon image {
    width: 38rpx;
    height: 38rpx;
}

.order-confirm-coupon .coupon-name {
    font-size: 28rpx;
    display: block;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.order-confirm-coupon .coupon-price {
    font-size: 28rpx;
    color: #ca141d;
    padding-left: 21rpx;
}

.order-fixed-address {
    padding: 0 34rpx;
    position: fixed;
    bottom: 93rpx;
    left: 0;
    right: 0;
    background-color: #faede5;
}

.order-fixed-address .fixed-address-title {
    font-size: 23rpx;
    color: #888;
    padding-right: 11rpx;
}

.order-fixed-address .fixed-address-cont {
    font-size: 25rpx;
    display: block;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.order-sys-total {
    padding-right: 0;
}

.order-sys-total .total-price {
    font-size: 23rpx;
    display: -ms-flexbox;
    display: box;
    display: -webkit-flex;
    display: flex;
    justify-content: flex-end;
}

.order-sys-total .total-price .order-sys-price {
    font-size: 32rpx;
    font-weight: bold;
    color: #ca141d;
}

.order-sys-total .total-price .order-sys-integral {
    color: #888;
}

.order-sys-total .action-btn .button-style-square {
    min-width: 188rpx;
    margin-left: 13rpx;
}

.ecWap-box-couponlist .ecWap-box-content {
    bottom: 0;
}

.ecWap-box-coupon {
}

.ecWap-box-coupon .coupon-list {
    height: 136rpx;
}

.ecWap-box-coupon .coupon-list .radiobox {
    padding-right: 21rpx;
}

.ecWap-box-coupon .disabled .radiobox .disabled+.state {
    opacity: 1;
}

.ecWap-box-coupon .coupon-list.disabled .coupon-cont {
    opacity: 0.3;
}

.ecWap-box-coupon .coupon-list .coupon-basic {
    font-size: 27rpx;
    font-weight: bold;
}

.ecWap-box-coupon .coupon-list .coupon-time {
    font-size: 23rpx;
    font-weight: normal;
    color: #888;
}

.ecWap-box-coupon .coupon-list .coupon-info {
    font-size: 25rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.coupon-empty {
    font-size: 27rpx;
    line-height: 136rpx;
    text-align: center;
}

.coupon-input {
    font-size: 27rpx;
    width: 100%;
    height: 136rpx;
    position: relative;
}

.coupon-input .system-flex-l {
    width: 100%;
}

.coupon-input .system-flex-l input {
    font-size: 27rpx;
    height: 63rpx;
    padding: 0 34rpx;
    border: 1px solid #eaeaea;
    border-radius: 32rpx;
    margin-right: 32rpx;
}

.coupon-input .system-fixed-r .button-style-1-big {
    height: 63rpx;
}

.coupon-input .coupon-tips {
    font-size: 23rpx;
    color: #ca141d;
    position: absolute;
    bottom: 5rpx;
    left: 0;
    right: 0;
}